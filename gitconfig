[commit]
	gpgsign = true
[alias]
	recommit = !git commit -eF $(git rev-parse --git-dir)/COMMIT_EDITMSG
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[init]
	defaultBranch = main
