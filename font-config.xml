<?xml version="1.0"?>
<!DOCTYPE fontconfig SYSTEM "fonts.dtd">
<fontconfig>
    <alias>
        <family>sans-serif</family>
        <prefer>
            <family>NotoSans</family>
            <family>NotoColorEmoji</family>
            <family>NotoEmoji</family>
        </prefer>
    </alias>
    <alias>
        <family>serif</family>
        <prefer>
            <family>NotoSerif</family>
            <family>NotoColorEmoji</family>
            <family>NotoEmoji</family>
        </prefer>
    </alias>
</fontconfig>
